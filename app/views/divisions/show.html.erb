<div class="row">
  <div class="small-8 large-8 columns">
    <header>
      <h1><%= @division.name %></h1>
    </header>
    <%= render partial: 'articles/island', collection: @articles %>
  </div>
  <div class="small-4 large-4 columns">
    <address><%= @division.address %></address>
    <p>
    <img src="http://maps.googleapis.com/maps/api/staticmap?center=<%= @division.latitude.to_s + ',' + @division.longitude.to_s %>&zoom=16&size=250x250&sensor=false&maptype=hybrid&markers=color:blue%7C<%= @division.latitude.to_s + ',' + @division.longitude.to_s %>">
  </div>
</div>
